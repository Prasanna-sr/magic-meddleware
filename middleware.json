{
    "static": {
        "enabled": true,
        "priority": 30,
        "module": {
            "name": "serve-static",
            "arguments": [ "path:./.build" ]
        }
    },

    "logger": {
        "enabled": true,
        "priority": 50,
        "module": {
            "name": "morgan",
            "arguments": [ "combined" ]
        }
    },

    "json": {
        "enabled": true,
        "priority": 60,
        "module": {
            "name": "body-parser",
            "method": "json"
        }
    },

    "urlencoded": {
        "enabled": true,
        "priority": 70,
        "module": {
            "name": "body-parser",
            "method": "urlencoded",
            "arguments": [{ "extended": true }]
        }
    },

    "cookieParser": {
        "enabled": true,
        "priority": 90,
        "module": {
            "name": "cookie-parser",
            "arguments": [ "keyboard cat" ]
        }
    },
    "routeA": {
        "enabled": true,
        "priority": 50,
        "module": {
            "name": "./routes",
            "method": "routeA"
        },
        "route": "/foo"
    },

    "routeB": {
        "enabled": true,
        "priority": 60,
        "module": {
            "name": "./routes",
            "method": "routeB"
        }
    },

    "routeC": {
        "enabled": true,
        "priority": 70,
        "module": {
            "name": "./routes",
            "method": "routeC"
        },
        "route": "bar"
    },

    "routeD": {
        "enabled": true,
        "priority": 80,
        "module": {
            "name": "./routes",
            "method": "routeD"
        },
        "route": "regex:^\/baz$"
    }

}
